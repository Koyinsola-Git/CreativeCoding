void setup(){
  size(600,400);
  textSize(50);
}
void draw(){
  background(255);
  fill(210,0,0);
  noStroke();
  circle(300,200,300);
  
  
  fill(255);
  noStroke();
  circle(300,200,180);
  
  fill(0,0,75);
  noStroke();
  rect(125,175,350,60);
  
  fill(255);
  noStroke();
  textAlign(CENTER);
  text("UNDERGROUND",300,220);
  
}
